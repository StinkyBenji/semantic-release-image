{
  "name": "semantic-release-image",
  "description": "Container image for running semantic release",
  "version": "1.0.0",
  "dependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/error": "^4.0.0",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "aggregate-error": "^5.0.0",
    "commitlint": "^19.3.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cosmiconfig": "^9.0.0",
    "debug": "^4.0.0",
    "env-ci": "^11.0.0",
    "execa": "^8.0.0",
    "figures": "^6.0.0",
    "find-versions": "^6.0.0",
    "get-stream": "^9.0.0",
    "git-log-parser": "^1.2.0",
    "hook-std": "^3.0.0",
    "hosted-git-info": "^7.0.0",
    "import-from-esm": "^1.3.1",
    "lodash-es": "^4.17.21",
    "marked": "^13.0.0",
    "marked-terminal": "^7.0.0",
    "micromatch": "^4.0.2",
    "p-each-series": "^3.0.0",
    "p-reduce": "^3.0.0",
    "read-package-up": "^11.0.0",
    "resolve-from": "^5.0.0",
    "semantic-release": "23.0.8",
    "semver": "^7.3.2",
    "semver-diff": "^4.0.0",
    "signale": "^1.2.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "ava": "6.1.3",
    "c8": "10.1.2",
    "clear-module": "4.1.2",
    "codecov": "3.8.3",
    "cz-conventional-changelog": "3.3.0",
    "dockerode": "4.0.2",
    "file-url": "4.0.0",
    "fs-extra": "11.2.0",
    "got": "14.4.1",
    "js-yaml": "4.1.0",
    "lockfile-lint": "4.13.2",
    "ls-engines": "0.9.1",
    "mockserver-client": "5.15.0",
    "nock": "13.5.4",
    "npm-run-all2": "6.2.0",
    "p-retry": "6.2.0",
    "prettier": "3.3.2",
    "publint": "0.2.8",
    "semantic-release": "23.0.8",
    "sinon": "18.0.0",
    "stream-buffers": "3.0.2",
    "tempy": "3.1.0",
    "testdouble": "3.20.2"
  },
  "engines": {
    "node": ">=20.8.1"
  },
  "files": [
    "bin",
    "docs",
    "lib",
    "index.d.ts",
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "automation",
    "changelog",
    "module",
    "package",
    "publish",
    "release",
    "semver",
    "version"
  ],
  "lockfile-lint": {
    "path": "package-lock.json",
    "type": "npm",
    "validate-https": true,
    "allowed-hosts": [
      "npm"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StinkyBenji/semantic-release-image.git"
  },
  "scripts": {
    "lint:prettier": "prettier --check \"*.{js,json,md}\" \".github/**/*.{md,yml}\" \"docs/**/*.md\" \"{bin,lib,test}/**/*.js\"",
    "lint:prettier:fix": "prettier --write \"*.{js,json,md}\" \".github/**/*.{md,yml}\" \"docs/**/*.md\" \"{bin,lib,test}/**/*.js\"",
    "lint:lockfile": "lockfile-lint",
    "lint:engines": "ls-engines",
    "lint:publish": "publint --strict"
  },
  "renovate": {
    "extends": [
      "github>semantic-release/.github:renovate-config"
    ]
  },
  "packageManager": "npm@10.8.1"
}
